{
  "$schema": "https://steeltoe.io/schema/latest/schema.json",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Steeltoe": "Information",
      "AcmeOrder": "Debug"
    }
  },
  "Steeltoe": {
    "Client": {
      "PostgreSql": {
        "Default": {
          "ConnectionString": "Host=localhost;Port=5432;Database=acme-order;Username=root;Password=rootpassword"
        }
      }
    }
  },
  "Eureka": {
    "Instance": {
      "HostName": "host.docker.internal"
    }
  },
  "AcmeServiceSettings": {
    "AuthUrl": "http://localhost:8090"
  }
}
