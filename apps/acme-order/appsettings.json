{
  "Spring": {
    "Application": {
      "Name": "acme-order"
    }
  },
  "DatabaseProvider": "Postgres",
  "DisableTokenValidation": "true",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information",
      "Steeltoe": "Information",
      "AcmeOrder": "Information"
    }
  },
  "Management": {
    "Endpoints": {
      "Actuator": {
        "Exposure": {
          "Include": [
            "*"
          ]
        },
        "Health": {
          "ShowComponents": "Always"
        }
      }
    }
  }
}
